<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/bg_yello" />
            <padding android:bottom="10sp" android:left="10sp" android:right="10sp" android:top="10sp" />
            <stroke android:width="1dp" android:color="@color/round_stroke" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/text_sub" />
            <padding android:bottom="10sp" android:left="10sp" android:right="10sp" android:top="10sp" />
            <stroke android:width="1dp" android:color="@color/round_stroke" />
        </shape>
    </item>
</selector>