<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="false" android:state_enabled="true">
        <shape>
            <solid android:color="@color/btn_sign" />
            <padding android:bottom="0dp" android:left="15sp" android:right="15sp" android:top="0dp" />
            <corners android:radius="3dp" />
            <stroke android:color="@color/btn_sign_stroke" android:width="1dp"/>
        </shape>
    </item>
    <item android:state_focused="false" android:state_pressed="true">
        <shape>
            <solid android:color="@color/btn_sign_press" />
            <padding android:bottom="0dp" android:left="15sp" android:right="15sp" android:top="0dp" />
            <corners android:radius="3dp" />
            <stroke android:color="@color/btn_sign_stroke" android:width="1dp"/>
        </shape>
    </item>
</selector>